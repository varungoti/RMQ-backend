{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "node dev.js",
    "start:no-check": "ts-node -r tsconfig-paths/register --transpile-only src/main.ts",
    "watch:ts": "tsc -w",
    "watch:node": "npx nodemon",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e-fixed": "node test/run-e2e-test.js",
    "db:seed": "ts-node -r tsconfig-paths/register src/seed.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:generate": "npm run typeorm -- migration:generate -d ormconfig.js",
    "migration:run": "npm run typeorm -- migration:run -d ormconfig.js",
    "migration:revert": "npm run typeorm -- migration:revert -d ormconfig.js",
    "check:hybrid-response": "node check-hybrid-response-usage.js",
    "test:verify-hybrid-fix": "node test/verify-controller-fix.js && node test/verify-new-hybrid-response.js",
    "lint:hybrid-response": "eslint -c .eslintrc.js --no-eslintrc --rulesdir=./test/eslint-rules --rule='no-incorrect-hybrid-response: error' '{src,test}/**/*.{js,ts}'",
    "precommit": "npm run check:hybrid-response && npm run lint",
    "test:diagnose": "node test-fix.js diagnose",
    "test:fix-imports": "node test-fix.js fix-imports",
    "test:safe": "node test-fix.js run",
    "test:check-hybrid": "node test-fix.js check-hybrid",
    "test:diagnose-file": "node test-diagnostic.js"
  },
  "dependencies": {
    "@apollo/gateway": "^2.10.0",
    "@apollo/server": "^4.11.3",
    "@nestjs/apollo": "^13.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/graphql": "^13.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.1.0",
    "@nestjs/typeorm": "^11.0.0",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "cache-manager": "^6.4.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "graphql": "^16.9.0",
    "graphql-type-json": "^0.3.2",
    "joi": "^17.13.3",
    "keyv": "^5.3.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.14.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "supabase": "^2.20.5",
    "typeorm": "^0.3.21",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/common": "^11.0.12",
    "@nestjs/core": "^11.0.12",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.12",
    "@nestjs/typeorm": "^11.0.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager": "^5.0.0",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/keyv": "^4.2.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "glob": "^11.0.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "madge": "^8.0.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-morph": "^25.0.1",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "wait-on": "^7.2.0",
    "webpack": "^5.98.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^entities/(.*)$": "<rootDir>/entities/$1"
    }
  }
}
